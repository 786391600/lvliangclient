  border-top-left-radius: 5px;
  border-bottom-right-radius: 5px;
  border-top-right-radius: 5px;
  font-size: 30rpx;
  font-weight: bold;
  color: #fff;
  margin-top: 120rpx;
}
.success_box{
  text-align: center;
  margin-top: 250rpx;
}